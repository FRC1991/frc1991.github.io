block navbar
nav.navbar.navbar-default.navbar-fixed-top(role = "navigation")
	.container
		.navbar-header
			//- Hamburger menu
			button.navbar-toggle.collapsed.pull-left(data-toggle = "collapse", data-target="#navbar-links")
				span.sr-only Toggle Navigation
				//- Repeated three times because each makes up a line in the hamburger
				span.icon-bar
				span.icon-bar
				span.icon-bar
			a.navbar-brand(href = "/")
				img(src = "assets/imgs/logo.jpg")
		div.collapse.navbar-collapse#navbar-links
			ul.nav.navbar-nav.navbar-right
				a.btn.btn-danger.navbar-btn(href = "/members") MEMBERS
				a.btn.btn-warning.navbar-btn(href = "https://quickclick.com/r/dd2l3", style="margin-left: 1em;") DONATIONS
				//- Build the navbar
				- var links = {"Home" : "/", "Sponsors": "/sponsors", "Creations": "/creations", "Media": "/media", "Awards": "/awards"}
				- for (var link in links)
					- url = links[link]
					li
						a(href = url) #{link}
